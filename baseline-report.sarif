      "tool": {
        "driver": {
          "name": "baseline-scan",
          "rules": [
            {
              "id": "html-popover",
              "name": "Popover attribute",
              "helpUri": "https://developer.mozilla.org/docs/Web/API/Popover_API",
              "shortDescription": {
                "text": "Popover attribute"
              },
              "fullDescription": {
                "text": "Fallback: use <dialog> or a custom popover component."
              },
              "properties": {
                "baseline": "partial"
              }
            },
            {
              "id": "structured-clone",
              "name": "structuredClone()",
              "helpUri": "https://developer.mozilla.org/docs/Web/API/structuredClone",
              "shortDescription": {
                "text": "structuredClone()"
              },
              "fullDescription": {
                "text": "Prefer structuredClone over deep-clone utilities; guard if targeting older browsers."
              },
              "properties": {
                "baseline": "yes"
              }
            },
            {
              "id": "url-canparse",
              "name": "URL.canParse()",
              "helpUri": "https://developer.mozilla.org/docs/Web/API/URL/canParse_static",
              "shortDescription": {
                "text": "URL.canParse()"
              },
              "fullDescription": {
                "text": "Fallback: try/catch new URL(...) for validation. (about 0% of your targets may lack support)"
              },
              "properties": {
                "baseline": "partial"
              }
            },
            {
              "id": "navigator-share",
              "name": "Web Share API",
              "helpUri": "https://developer.mozilla.org/docs/Web/API/Navigator/share",
              "shortDescription": {
                "text": "Web Share API"
              },
              "fullDescription": {
                "text": "Guard: if (navigator.share) { await navigator.share(...) } else { fallback } (about 18% of your targets may lack support)"
              },
              "properties": {
                "baseline": "partial"
              }
            },
            {
              "id": "array-prototype-at",
              "name": "Array.prototype.at()",
              "helpUri": "https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Array/at",
              "shortDescription": {
                "text": "Array.prototype.at()"
              },
              "fullDescription": {
                "text": "Fallback: use arr[index >= 0 ? index : arr.length + index] for negatives."
              },
              "properties": {
                "baseline": "partial"
              }
            },
            {
              "id": "promise-any",
              "name": "Promise.any()",
              "helpUri": "https://developer.mozilla.org/docs/Web/JavaScript/Reference/Global_Objects/Promise/any",
              "shortDescription": {
                "text": "Promise.any()"
              },
              "fullDescription": {
                "text": "Fallback: emulate with Promise.race on wrapped promises or a small polyfill."
              },
              "properties": {
                "baseline": "partial"
              }
            },
            {
              "id": "urlpattern",
              "name": "URLPattern",
              "helpUri": "https://developer.mozilla.org/docs/Web/API/URL_Pattern_API",
              "shortDescription": {
                "text": "URLPattern"
              },
              "fullDescription": {
                "text": "Fallback: use the urlpattern-polyfill or Regex-based matching."
              },
              "properties": {
                "baseline": "partial"
              }
            },
            {
              "id": "view-transitions",
              "name": "View Transitions API",
              "helpUri": "https://developer.mozilla.org/docs/Web/API/Document/startViewTransition",
              "shortDescription": {
                "text": "View Transitions API"
              },
              "fullDescription": {
                "text": "Guard: if ('startViewTransition' in document) { ... } else { ... } (about 22% of your targets may lack support)"
              },
              "properties": {
                "baseline": "partial"
              }
            },
            {
              "id": "async-clipboard",
              "name": "Async Clipboard API",
              "helpUri": "https://developer.mozilla.org/docs/Web/API/Clipboard_API",
              "shortDescription": {
                "text": "Async Clipboard API"
              },
              "fullDescription": {
                "text": "Guard: if (navigator.clipboard?.writeText) { await navigator.clipboard.writeText(...) } else { /* fallback */ }"
              },
              "properties": {
                "baseline": "partial"
              }
            },
            {
              "id": "html-dialog",
              "name": "<dialog> element",
              "helpUri": "https://developer.mozilla.org/docs/Web/HTML/Element/dialog",
              "shortDescription": {
                "text": "<dialog> element"
              },
              "fullDescription": {
                "text": "Provide a dialog polyfill or non-modal fallback when unsupported; ensure accessible focus management."
              },
              "properties": {
                "baseline": "partial"
              }
            },
            {
              "id": "import-maps",
              "name": "Import Maps",
              "helpUri": "https://developer.mozilla.org/docs/Web/HTML/Element/script/type/importmap",
              "shortDescription": {
                "text": "Import Maps"
              },
              "fullDescription": {
                "text": "Guard or provide bundler fallback for environments without native import maps."
              },
              "properties": {
                "baseline": "partial"
              }
            },
            {
              "id": "loading-lazy-attr",
