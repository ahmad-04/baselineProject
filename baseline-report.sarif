              "shortDescription": {
                "text": "Lazy loading attribute"
              },
              "fullDescription": {
                "text": "Use for non-critical images/iframes; set hero media to eager to protect LCP."
              },
              "properties": {
                "baseline": "partial"
              }
            },
            {
              "id": "css-has",
